{
  "name": "consultation-kit-dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dashboard.consultationkit.com",
  "proxy": "http://consultationkit.app:5000",
  "devDependencies": {
    "react-scripts": "0.9.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.4"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-modal": "^1.7.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "deploy:dev": "npm run build:dev && aws s3 sync ./build s3://dashboard-dev.consultationkit.com --acl=public-read",
    "deploy:production": "npm run build:production && aws s3 sync ./build s3://dashboard.consultationkit.com --acl=public-read"
  }
}
